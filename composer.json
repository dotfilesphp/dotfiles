{
    "name": "toni/dotfiles",
    "description": "Dotfiles Installer",
    "type": "project",
    "license": "MIT",
    "authors": [
        {
            "name": "Anthonius Munthi",
            "email": "me@itstoni.com"
        }
    ],
    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "vim/vundle",
                "version": "0.10.2",
                "dist": {
                    "url": "https://github.com/VundleVim/Vundle.vim/archive/v0.10.2.zip",
                    "type": "zip"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "bash-it/bash-it",
                "version": "1.0.0",
                "dist": {
                    "url": "https://codeload.github.com/Bash-it/bash-it/zip/master",
                    "type": "zip"
                }
            }
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "symfony/finder": "^4.0",
        "symfony/console": "^4.0",
        "vim/vundle": "0.10.2",
        "bash-it/bash-it": "^1.0"
    },
    "autoload": {
        "psr-4": { "Toni\\Dotfiles": "src" }
    }
}
