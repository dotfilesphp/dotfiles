{
    "name": "dotfiles/dotfiles",
    "description": "Dotfiles Installer",
    "type": "project",
    "license": "MIT",
    "authors": [
        {
            "name": "Anthonius Munthi",
            "email": "me@itstoni.com"
        }
    ],
    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "vim/vundle",
                "version": "0.10.2",
                "dist": {
                    "url": "https://github.com/VundleVim/Vundle.vim/archive/v0.10.2.zip",
                    "type": "zip"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "bash-it/bash-it",
                "version": "1.0.0",
                "dist": {
                    "url": "https://codeload.github.com/Bash-it/bash-it/zip/master",
                    "type": "zip"
                }
            }
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": "^7.1 | ^7.2",
        "symfony/finder": "^4.0",
        "symfony/console": "^4.0",
        "vim/vundle": "0.10.2",
        "bash-it/bash-it": "^1.0",
        "symfony/filesystem": "^4.0",
        "symfony/process": "^4.0",
        "seld/phar-utils": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "Toni\\Dotfiles\\": "src/",
            "Tests\\Unit\\Toni\\Dotfiles\\": "tests/unit",
            "Tests\\Functional\\Toni\\Dotfiles\\": "tests/functional"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
