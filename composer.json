{
    "name": "dotfiles/dotfiles",
    "description": "Dotfiles Main Development Repository",
    "type": "project",
    "license": "MIT",
    "authors": [
        {
            "name": "Anthonius Munthi",
            "email": "me@itstoni.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": "^7.1 | ^7.2",
        "symfony/finder": "^4.0",
        "symfony/console": "^4.0",
        "symfony/filesystem": "^4.0",
        "symfony/process": "^4.0",
        "monolog/monolog": "^1.23",
        "symfony/event-dispatcher": "^4.0",
        "symfony/config": "^4.0",
        "symfony/dependency-injection": "^4.0",
        "symfony/yaml": "^4.0",
        "symfony/dotenv": "^4.0",
        "psy/psysh": "^0.8.18"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0",
        "seld/phar-utils": "^1.0",
        "behat/behat": "^3.4"
    },
    "autoload": {
        "psr-4": {
            "Dotfiles\\Core\\": "src/Core",
            "Dotfiles\\Plugins\\": "src/Plugins"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Dotfiles\\Behat\\": "src/Behat"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
